{
  "name": "CleanNodeApi",
  "version": "1.1.0",
  "main": "index.js",
  "author": "Renato Castro <renatocastro02@hotmail.com>",
  "license": "MIT",
  "scripts": {
    "start": "node dist/main/server.js",
    "build": "rimraf dist && tsc",
    "up": "npm run build && docker-compose up",
    "test": "jest --passWithNoTests --runInBand",
    "test:silent": "yarn test -- --silent --noStackTrace",
    "test:watch": "yarn test -- --watch -c jest-unit-config.js",
    "test:integration": "yarn test -- --watch -c jest-integration-config.js",
    "test:staged": "yarn test:silent -- --findRelatedTests",
    "test:ci": "yarn test:silent  -- --coverage"
  },
  "devDependencies": {
    "@shelf/jest-mongodb": "^1.2.4",
    "@types/bcrypt": "3.0.0",
    "@types/express": "^4.17.11",
    "@types/jest": "^26.0.23",
    "@types/jsonwebtoken": "^8.5.3",
    "@types/mongodb": "^3.6.15",
    "@types/node": "^15.0.2",
    "@types/supertest": "^2.0.11",
    "@types/validator": "^13.1.3",
    "@typescript-eslint/eslint-plugin": "^4.23.0",
    "@typescript-eslint/parser": "^4.23.0",
    "eslint": "^7.26.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-typescript": "^2.4.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-import-helpers": "^1.1.0",
    "eslint-plugin-prettier": "^3.4.0",
    "git-commit-msg-linter": "^3.1.0",
    "husky": "^6.0.0",
    "jest": "^26.6.3",
    "lint-staged": "^11.0.0",
    "prettier": "^2.3.0",
    "rimraf": "^3.0.2",
    "supertest": "^6.1.3",
    "ts-jest": "^26.5.6",
    "typescript": "^4.2.4"
  },
  "dependencies": {
    "bcrypt": "^5.0.1",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "mongodb": "^3.5.1",
    "validator": "^13.6.0"
  },
  "engines": {
    "node": "14.x"
  }
}
